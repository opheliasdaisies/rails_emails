<h1>Listing emails</h1>

<table>
  <tr>
    <th>To</th>
    <th>From</th>
    <th>Subject</th>
    <th>Body</th>
  </tr>

  <% @emails.each do |email| %>
    <tr>
      <td><%= email.to %></td>
      <td><%= email.from %></td>
      <td><%= email.subject %></td>
      <td><%= email.body %></td>
    </tr>
  <% end%>
</table>

<br>

<%= link_to 'New Email', new_email_path %>